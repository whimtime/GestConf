<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from servizioutentiService WSDL service -->

  <service name="servizioutenti" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://servizi.gestioneconferenzews.com"/>
      <parameter name="wsdlServiceElement" value="servizioutentiService"/>
      <parameter name="schemaQualified" value="http://servizi.gestioneconferenzews.com"/>
      <parameter name="wsdlServicePort" value="servizioutenti"/>
      <parameter name="className" value="com.gestioneconferenzews.servizi.ServizioutentiSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="servizioutenti"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="verificautentelogin" qname="operNS:verificautentelogin" xmlns:operNS="http://servizi.gestioneconferenzews.com" returnQName="retNS:verificautenteloginReturn" xmlns:retNS="http://servizi.gestioneconferenzews.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://servizi.gestioneconferenzews.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://servizi.gestioneconferenzews.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="verificautentelogin"/>

  </service>
</deployment>
